<body class="bg-light">
  {{> nav}}
  <div class="container mt-3">
    <div class="row">
      {{#each userData}}
      <div class="col-12 col-md-6">
        <div class="card mt-5 mx-auto">
          <div class="row">
            <div class="col-sm-6 bg-light ">
              <a href="#" class="btn btn-primary text-center text-white disabled">See Profile :-</a>
              <h5 class="card-title mt-2">Name -> {{name}}</h5>
              <h5 class="card-text">Email -> {{email}}</h5>
              <h5 class="card-text">ContactNo -> {{mobileNo}}</h5>
            </div>
            <div class="col-sm-6 ">
              <div class="card-body">

                <img class="card-img-top" src="img_avatar5.png" alt="Card image">
              </div>

            </div>
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>



  {{> footer}}
</body>

</html>