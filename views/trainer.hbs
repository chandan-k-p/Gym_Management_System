<body class="bg-light">

    {{> nav}}



    <div class="container mt-3">
        <div class="row">
            {{#each userData}}
            <div class="col-12 col-md-6">
                <div class="card mt-5 mx-auto">
                    <div class="row">
                        <div class="col-sm-6">
                            <img class="card-img-top" src="img_avatar1.png" alt="Card image">
                        </div>
                        <div class="col-sm-6 bg-info text-white">
                            <div class="card-body">
                                <a href="#" class="btn btn-primary text-white disabled">See Profile :-</a>
                                <h5 class="card-title mt-2">Name -> {{name}}</h5>
                                <h5 class="card-text">Email -> {{email}}</h5>
                                <h5 class="card-text">ContactNo -> {{mobileNo}}</h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>








    {{> footer}}
</body>